{
  "name": "react-demo",
  "version": "1.0.0",
  "description": "基于webpack的react学习与演示项目",
  "main": "index.js",
  "scripts": {
    "build": "set NODE_ENV=production && webpack --progress",
    "build:devDll": "set NODE_ENV=development && webpack --progress --config webpack.dll.config.js",
    "build:proDll": "set NODE_ENV=production && webpack --progress --config webpack.dll.config.js",
    "generate": "plop --plopfile generators/index.js",
    "start": "set NODE_ENV=development && webpack-dev-server --hot --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "webpack"
  ],
